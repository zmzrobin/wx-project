.pn{position:static;}
.zoom{zoom:1;}
.hidden{visibility:hidden;}
.none{display:none;}
.block{display:block;}
.in_block{display:inline-block;}
.v_top{ vertical-align:top;}
.v_mid{ vertical-align:middle;}
.text_ind20{ text-indent:2em;}
.ov_hid{ overflow:hidden}
.one_lin{ overflow:hidden; display:inline-block;white-space: nowrap; text-overflow: ellipsis;}
.two_lin{ height:40rpx; line-height:40rpx; overflow:hidden; display:inline-block;}
.nowrap{white-space: nowrap;}
.tb_fix{table-layout: fixed;}


