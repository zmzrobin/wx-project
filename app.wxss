/**app.wxss**/
@import 'lib/iconfont.wxss'
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.body {
  position: absolute;
  top:0;
  right:0;
  left:0;
  bottom:0;
  height: 1110rpx;
}
view{
  box-sizing: border-box;
}
/*color*/
.bg-co-w{ background-color:#FFF !important;}
.co-b{ color:#000 !important;}
.co-w{ color:#fff !important;}
.co-red{ color:#ff0000 !important;}
.co-gray{ color:#bbb !important;}
.co-333{color: #3333 !important;}
.co-666{color: #666 !important;}
.co-999{color: #999 !important;}

/*flex*/
.flex{display: flex;}
.main-center{justify-content: center;}
.main-between{justify-content: space-between;}
.main-around{justify-content: space-around}
.cross-center{align-items: center;}
.cross-stretch{align-items: stretch;}
.self-end{align-self: flex-end;}
.self-start{align-self: flex-start;}
.flex-auto{flex: auto;}
.flex-none{flex:none;}
.flex1{flex: 1;}

